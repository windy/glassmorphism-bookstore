# Use `rake secret` to generate one key, Only needed in production environment
SECRET_KEY_BASE: ''

# Your published app's HOST, Used by emailer/payment in production environment
DOMAIN: ''
# Your published app's PROTOCOL, Used by emailer/payment in production environment
PROTOCOL: 'https'

# Browser base title
APPNAME: 'ClackyAI APP'

# OAuth provider credentials generated by authentication generator
# Set enabled: 'true' to activate each OAuth provider (use strings to avoid Figaro warnings)

# Google OAuth
GOOGLE_OAUTH_ENABLED: 'false'
GOOGLE_CLIENT_ID: 'your_google_client_id'
GOOGLE_CLIENT_SECRET: 'your_google_client_secret'

# Facebook OAuth
FACEBOOK_OAUTH_ENABLED: 'false'
FACEBOOK_APP_ID: 'your_facebook_app_id'
FACEBOOK_APP_SECRET: 'your_facebook_app_secret'

# Twitter OAuth
TWITTER_OAUTH_ENABLED: 'false'
TWITTER_API_KEY: 'your_twitter_api_key'
TWITTER_API_SECRET: 'your_twitter_api_secret'

# GitHub OAuth
GITHUB_OAUTH_ENABLED: 'false'
GITHUB_CLIENT_ID: 'your_github_client_id'
GITHUB_CLIENT_SECRET: 'your_github_client_secret'
# OAuth provider credentials generated end

# Stripe Configuration
STRIPE_PUBLISHABLE_KEY: "pk_test_your_publishable_key_here"
STRIPE_SECRET_KEY: "sk_test_your_secret_key_here"
STRIPE_WEBHOOK_SECRET: "whsec_your_webhook_secret_here"
